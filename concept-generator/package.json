{
  "name": "impower-concept-generator",
  "author": "Impower Games",
  "version": "0.1.0",
  "description": "The scripts and relevancy algorithms used for Impower's Random Concept Generators",
  "keywords": [
    "procedural generation",
    "procedural generator",
    "procedural",
    "generator",
    "procgen",
    "randomizer"
  ],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "tsc && node dist/index.js",
    "curate": "ts-node scripts/curate.ts",
    "moderate": "ts-node scripts/moderate.ts",
    "keywords": "ts-node scripts/keywords.ts",
    "similar": "ts-node scripts/similar.ts",
    "related": "ts-node scripts/related.ts",
    "vector": "ts-node scripts/vector.ts",
    "clean": "ts-node scripts/clean.ts",
    "difference": "ts-node scripts/difference.ts",
    "concept": "ts-node scripts/concept.ts",
    "closest": "ts-node scripts/closest.ts",
    "map": "ts-node scripts/map.ts",
    "sort": "ts-node scripts/sort.ts",
    "unpack": "ts-node scripts/unpack.ts",
    "average": "ts-node scripts/average.ts",
    "missing": "ts-node scripts/missing.ts",
    "suggest": "npm run keywords && npm run vector && npm run related",
    "prep": "npm run sort && npm run clean && npm run unpack",
    "dev": "npm run prep && npm run curate"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.0",
    "@types/node": "^16.11.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "cli-progress": "^3.9.0",
    "fastest-levenshtein": "^1.0.12",
    "yaml": "^2.2.2"
