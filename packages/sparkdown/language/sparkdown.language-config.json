{
  "comments": {
    "lineComment": "//",
    "blockComment": [
      "/*",
      "*/"
    ]
  },
  "brackets": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ]
  ],
  "autoClosingPairs": [
    {
      "open": "- {",
      "close": "  }"
    },
    {
      "open": "{",
      "close": "}"
    },
    {
      "open": "[",
      "close": "]"
    },
    {
      "open": "(",
      "close": ")"
    },
    {
      "open": "<",
      "close": ">",
      "notIn": [
        "DialogueCharacterPosition"
      ]
    },
    {
      "open": "\"",
      "close": "\""
    },
    {
      "open": "=== ",
      "close": " ==="
    },
    {
      "open": "== ",
      "close": " =="
    },
    {
      "open": "~~",
      "close": "~~"
    },
    {
      "open": "::",
      "close": "::"
    },
    {
      "open": "/*",
      "close": "*/",
      "notIn": [
        "StringLiteral"
      ]
    }
  ],
  "surroundingPairs": [
    [
      "{",
      "}"
    ],
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ],
    [
      "<",
      ">"
    ],
    [
      "\"",
      "\""
    ],
    [
      "*",
      "*"
    ],
    [
      "_",
      "_"
    ],
    [
      "~",
      "~"
    ],
    [
      ":",
      ":"
    ],
    [
      "^",
      "^"
    ]
  ],
  "wordChars": "_'",
  "wordPattern": {
    "pattern": "(\\p{L}|\\p{N}|\\p{Mn})(((\\p{L}|\\p{N}|\\p{Mn})|[_]|['])?(\\p{L}|\\p{N}|\\p{Mn}))*",
    "flags": "u"
  },
  "onEnterRules": [
    {
      "beforeText": "^.*[-]\\s+[{]\\s*$",
      "afterText": "\\s*[}]",
      "action": {
        "indent": "indentOutdent",
        "appendText": "\t- "
      }
    },
    {
      "beforeText": "^.*[{]\\s*$",
      "afterText": "\\s*[}]",
      "action": {
        "indent": "indentOutdent",
        "appendText": "- "
      }
    },
    {
      "beforeText": "^\\s*[-]\\s+.*?(?<![:])\\s*$",
      "previousLineText": "(?=^.*\\s*[{].*)(?=(?!\\s*[}]))|(?=^.*\\s*[-]\\s+.*?(?<![:])\\s*$)",
      "action": {
        "indent": "none",
        "appendText": "- "
      }
    },
    {
      "beforeText": "^.*[{][^}]*$",
      "afterText": "\\s*[}]",
      "action": {
        "indent": "indentOutdent"
      }
    },
    {
      "beforeText": "^\\s*([+]\\s+)+.*?([-][>]).*?$",
      "action": {
        "indent": "none"
      }
    },
    {
      "beforeText": "^\\s*([*]\\s+)+.*?([-][>]).*?$",
      "action": {
        "indent": "none"
      }
    },
    {
      "beforeText": "^\\s*([-]\\s+)+.*?([-][>]).*?$",
      "action": {
        "indent": "none"
      }
    },
    {
      "beforeText": "\\s*([+]\\s+)+.*?(?![-][>]).*?$",
      "action": {
        "indent": "indent"
      }
    },
    {
      "beforeText": "\\s*([*]\\s+)+.*?(?![-][>]).*?$",
      "action": {
        "indent": "indent"
      }
    },
    {
      "beforeText": "\\s*([-]\\s+)+.*?(?![-][>]).*?$",
      "action": {
        "indent": "indent"
      }
    },
    {
      "beforeText": "^.*?([-][>])\\s*\\S+\\s*$",
      "action": {
        "indent": "outdent"
      }
    },
    {
      "beforeText": "\\[\\s*$",
      "afterText": "^\\s*\\].*$",
      "action": {
        "indent": "indentOutdent"
      }
    },
    {
      "beforeText": "\\[\\s*$",
      "afterText": "^(?!\\s*\\])",
      "action": {
        "indent": "indent"
      }
    },
    {
      "beforeText": "\\(\\s*$",
      "afterText": "^\\s*\\).*$",
      "action": {
        "indent": "indentOutdent"
      }
    },
    {
      "beforeText": "\\(\\s*$",
      "afterText": "^(?!\\s*\\))",
      "action": {
        "indent": "indent"
      }
    },
    {
      "beforeText": "^\\s*(?![%]\\s+).*?[:]\\s*$",
      "action": {
        "indent": "indent"
      }
    },
    {
      "beforeText": "^\\s*(?:[-])\\s*$",
      "action": {
        "indent": "indent"
      }
    },
    {
      "beforeText": "^\\s*(?:[-])\\s+.*(?:\\s[=]\\s).*$",
      "action": {
        "indent": "indent"
      }
    },
    {
      "beforeText": "^\\s*(?:[%]\\s+).*?[\\\\]\\s*$",
      "action": {
        "indent": "indent"
      }
    },
    {
      "beforeText": "^\\s*(?:[$]\\s+).*?[\\\\]\\s*$",
      "action": {
        "indent": "indent"
      }
    },
    {
      "beforeText": "^\\s*(?:[!]\\s+).*?[\\\\]\\s*$",
      "action": {
        "indent": "indent"
      }
    },
    {
      "beforeText": "^\\s*(?:[@]\\s+.*?[:]).*?[\\\\]\\s*$",
      "action": {
        "indent": "indent"
      }
    },
    {
      "beforeText": "^\\s*$",
      "action": {
        "indent": "none",
        "deleteText": "  "
      }
    },
    {
      "beforeText": "^\\s*",
      "action": {
        "indent": "none"
      }
    }
  ],
  "indentationRules": {
    "decreaseIndentPattern": "^\\s*[\\}\\]\\)].*$",
    "increaseIndentPattern": "^.*(\\{[^}]*|\\([^)]*|\\[[^\\]]*)$",
    "unIndentedLinePattern": "^(\\t|[ ])*[ ]\\*[^/]*\\*/\\s*$|^(\\t|[ ])*[ ]\\*/\\s*$|^(\\t|[ ])*[ ]\\*([ ]([^\\*]|\\*(?!/))*)?$",
    "indentNextLinePattern": "^((.*=>\\s*)|((.*[^\\w]+|\\s*)(if|while|for)\\s*\\(.*\\)\\s*))$"
  },
  "folding": {
    "markers": {
      "start": "^\\s*==.*?$",
      "end": "^\\s*==.*?$"
    }
  }
}