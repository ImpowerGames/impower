{
  "name": "functions",
  "scripts": {
    "lint": "eslint \"src/**/*\"",
    "build": "tsc",
    "sort": "cd ../title-generator && npm run sort && cd ../admin",
    "clean": "cd ../title-generator && npm run clean && cd ../admin",
    "unpack": "cd ../title-generator && npm run unpack && cd ../admin",
    "prep": "cd ../title-generator && npm run prep && cd ../admin",
    "average": "cd ../title-generator && npm run average && cd ../admin",
    "config": "npm run config-dev",
    "config-dev": "ts-node scripts/config/config.ts dev",
    "config-test": "ts-node scripts/config/config.ts test",
